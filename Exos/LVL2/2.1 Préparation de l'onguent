import algorea.Scanner;

class Main {

static Scanner entrée = new Scanner(System.in);
    public static void main(String[] args) {

        int numIngrédients=entrée.nextInt();
        int[] listeIngrédients={500,180,650,25,666,42,421,1,370,211};

        System.out.println(listeIngrédients[numIngrédients]);
    }
}