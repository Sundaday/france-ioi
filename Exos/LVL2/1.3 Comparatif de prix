

import algorea.Scanner;

class Main {
    static Scanner entrée = new Scanner(System.in);

    public static void main(String[] args) {

        double nbLegumes =entrée.nextInt();

        for (int i = 0; i < nbLegumes; i++) {
            double poids =entrée.nextFloat();
            double age = entrée.nextFloat();
            double prix = entrée.nextFloat();
            double prixKg = prix / poids;
            System.out.println(prixKg);
        }
    }
}